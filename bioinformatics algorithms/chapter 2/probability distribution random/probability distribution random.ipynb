    "import random\n",
    "\n",
    "def Random(p):\n",
    "    r = random.random()\n",
    "    C = sum(p)\n",
    "    \n",
    "    s = 0.0\n",
    "    for i in range(len(p)):\n",
    "        s += (p[i] / C)\n",
    "        if s >= r:\n",
    "            return i"
